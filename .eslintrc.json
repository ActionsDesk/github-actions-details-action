{
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true
  },
  "plugins": ["github"],
  "extends": ["plugin:github/recommended"],
  "rules": {
    "camelcase": [
      "error",
      {
        "allow": ["commit_id", "issue_number", "pull_number", "subject_type", "per_page"]
      }
    ],
    "no-console": "error"
  }
}
